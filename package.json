{
  "name": "koinos-sdk-as-examples",
  "version": "0.4.2",
  "main": "assembly/index.ts",
  "license": "MIT",
  "author": "Roamin Roe <roamin@tuta.io>",
  "repository": "https://github.com/roaminroe/koinos-sdk-as-examples",
  "dependencies": {
    "commander": "^9.0.0"
  },
  "devDependencies": {
    "@as-covers/core": "^0.2.1",
    "@as-pect/cli": "^6.2.4",
    "@roaminroe/as-proto-gen": "^0.4.2",
    "@roaminroe/protoc": "^2.4.0",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "assemblyscript": "^0.19.22",
    "eslint": "^8.7.0",
    "koinos-abi-proto-gen": "^0.1.11",
    "koinos-as-gen": "^0.4.8",
    "koinos-mock-vm": "^1.1.0",
    "koinos-proto-as": "^0.4.1",
    "koinos-sdk-as": "^0.4.10",
    "koinos-sdk-as-cli": "^0.1.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5",
    "yaml": "^2.0.0-11"
  },
  "scripts": {
    "lint": "eslint --max-warnings 0 --ext ts \"*/assembly/**/*.ts\"",
    "buildCalculator": "koinos-sdk-as-cli build-all calculator release calculator.proto && koinos-sdk-as-cli run-tests calculator",
    "buildChat": "koinos-sdk-as-cli build-all chat release chat.proto",
    "buildControlledContractUpdate": "koinos-sdk-as-cli build-all --generate_authorize controlled_contract_update release update.proto",
    "buildImmutable": "koinos-sdk-as-cli build-all immutable release immutable.proto",
    "buildNft": "koinos-sdk-as-cli build-all nft release nft.proto",
    "buildStaking": "koinos-sdk-as-cli build-all staking release staking.proto",
    "buildToken": "koinos-sdk-as-cli build-all token release token.proto && koinos-sdk-as-cli run-tests token",
    "buildTransactionStorage": "koinos-sdk-as-cli build transaction_storage release && koinos-sdk-as-cli run-tests transaction_storage",
    "buildAll": "yarn lint && yarn buildCalculator && yarn buildChat && yarn buildControlledContractUpdate && yarn buildImmutable && yarn buildNft && yarn buildStaking && yarn buildToken && yarn buildTransactionStorage"
  }
}
